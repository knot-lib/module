  "description": "Module system for kNot Framework",
  "keywords": ["PHP","PHP7","Module","library","kNot Framework"],
  "homepage": "https://github.com/knot-lib/module",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "stk2k/file": "~0.1",
    "knot-lib/support": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "php-coveralls/php-coveralls": "^2.0",
    "mikey179/vfsstream": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "KnotLib\\Module\\": "src/"
