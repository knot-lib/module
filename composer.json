{
  "name": "knot-lib/module",
  "description": "Module system for kNot Framework",
  "keywords": ["PHP","PHP7","Module","library","kNot Framework"],
  "homepage": "https://github.com/knot-lib/module",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "stk2k/file-system": "~0.1",
    "knot-lib/support": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0",
    "mikey179/vfsstream": "1.6.8"
  },
  "autoload": {
    "psr-4": {
      "knotlib\\module\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "knotlib\\module\\test\\": "test/",
      "knotlib\\module\\test\\classes\\": "test/classes/",
      "knotlib\\module\\demo\\": "demo/include/"
    }
  }
}